#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <string.h>
#pragma warning(disable : 4996)
int arr[100];

/*
* merge sort algorithm
* 9 7 5 3 1  |
* 9 7 5		 |			3 1
* 9 7 | 5	 |	3 | 1
  9 | 7 | 5	 |	(3,1)
  (7,9) | 5  | (3,1)
  (5,7,9)	 | (3,1)
  
  (1,3,5,7,9)


  (9,7,5,3,1)
  (9,7,5) , (3,1)
  (9,7), (5), | (3), (1)
  (9), (7), (5) | (1,3)
*/
void	rmergesortarray(int* arr, int l, int r)
{
	int m;

	if ()
	{

		mergearray(arr, l, m , r)
	}
	else
		return;

}

void	mergesortarray(int* arr, int n)
{
	rmergesortarray(arr, 0, n-1);

}


int main()
{
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		scanf("%d", &arr[i]);
	}
	mergesortarray(arr, n);


}
